{
  "compilerOptions": {
    "allowJs": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "strict": true,
    "isolatedModules": true,
    "target": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "skipLibCheck": true,
    "paths": {
      "ziggy-js": [
        "./vendor/tightenco/ziggy"
      ],
      "@styles": [
        "./resources/js/assets/styles"
      ],
      "@styles/*": [
        "./resources/js/assets/styles/*"
      ],
      "@helpers": [
        "./resources/js/helpers"
      ],
      "@helpers/*": [
        "./resources/js/helpers/*"
      ],
      "@interfaces": [
        "./resources/js/interfaces"
      ],
      "@interfaces/*": [
        "./resources/js/interfaces/*"
      ],
      "@layouts": [
        "./resources/js/layouts"
      ],
      "@layouts/*": [
        "./resources/js/layouts/*"
      ],
      "@types": [
        "./resources/js/types"
      ],
      "@types/*": [
        "./resources/js/types/*"
      ],
      "@pages": [
        "./resources/js/pages"
      ],
      "@pages/*": [
        "./resources/js/pages/*"
      ]
    }
  },
  "include": [
    "resources/js/**/*.ts",
    "resources/js/**/*.d.ts",
    "resources/js/**/*.vue"
  ]
}
